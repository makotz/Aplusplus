<div id="calendar"></div>
<script>
$(document).ready(function() {
    $('#calendar').html("")
    $('#calendar').fullCalendar({
        header: {
            left: 'today',
            center: 'prev title next',
            right: ''
        },

        defaultView :'month',
        height: 600,
        events: '/events.json'
    });
});
</script>
